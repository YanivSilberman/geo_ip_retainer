  "dependencies": {
    "babel-core": "^6.26.3",
    "babel-plugin-transform-es2015-arrow-functions": "^6.22.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "babel-register": "^6.26.0",
    "bluebird": "^3.5.1",
    "cloudinary": "^1.11.0",
    "cookie-parser": "~1.4.3",
    "cron": "^1.3.0",
    "debug": "~2.6.9",
    "dotenv": "^5.0.1",
    "express": "~4.16.0",
    "fs": "0.0.1-security",
    "http-errors": "~1.6.2",
    "https": "^1.0.0",
    "jade": "~1.11.0",
    "moment": "^2.22.1",
    "mongoose": "^5.1.2",
    "morgan": "~1.9.0",
